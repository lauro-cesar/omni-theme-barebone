<form class="form-horizontal" name="formCep_<%- id %>">
  <div class="form-group">
    <label class="col-sm-4 control-label">Cep de destino</label>
    <div class="col-sm-8">

      <input type="text" name="zipCode" class="form-control"  placeholder="CEP">

		<p class="text-danger zipCode_EmptyMessage">
		Preencha com um CEP válido.
		</p>


    </div>

  </div>

  <div class="form-group">
    <label  class="col-sm-4 control-label">Qt itens</label>
    <div class="col-sm-8">
      <input type="text" class="form-control"  placeholder="CEP" name="itemQT" value="<%- itemQT %>">
		<p class="text-danger itemQT_EmptyMessage">
		Preencha com um valor válido.
		</p>
    </div>

  </div>

  <div class="form-group">
    <div class="col-sm-offset-4 col-sm-4">
   <a class="btn btn-info btn-sm omni_shipping_estimate omni-action" role="button">
  Calcular frete
  </a>
    </div>
    <div class="col-sm-4">
      <a class="btn btn-primary btn-sm omni_dismiss omni-action scrolltoup" role="button">
   	Fechar
  </a>
    </div>


  </div>
</form>



  <span class="loading">
  Carregando estimativas de frete.
  <img width="32px" src="{{theme_cdn_dir}}/static/img/loading.gif">
  </span>

</script>


<script type="text/template" id="shipping_calculator_method_render_template"> 



<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title"><%- label %></h3>
  </div>
  <div class="panel-body">
   <span class="shippingPriceContainer"> 
   	
   </span>
  </div>
</div>







</script>




<script type="text/template" id="user_account_welcome_link">

<% if (connected) { %>
	Seja bem vindo <%- accountName %>
<% }  %>
	
</script>


<script type="text/template" id="user_account_widget">
  
<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Dados da conta</h3>
  </div>
  <div class="panel-body">


<form class="form-horizontal form-account"  name="formAccount_<%- sessionID %>">
  

  <div class="form-group">
  <label for="accountName" class="col-sm-2 control-label" >Nome completo</label>
  
  <div class="col-sm-10"> 
    <input type="text" name="accountName" id="accountName" value="<%- accountName %>" class="form-control" aria-describedby="helpBlock">
    <span id="helpBlock" class="help-block">Seu nome completo</span>
  </div>
  </div>

  <div class="form-group">
  <label for="accountEmail" class="col-sm-2 control-label" >Email de contato</label>
  
  <div class="col-sm-10"> 
    <input type="text" name="accountEmail" id="accountEmail" value="<%- accountEmail %>" class="form-control" aria-describedby="helpBlock">
    <span id="helpBlock" class="help-block">Email para receber os avisos de acompanhamento de pedidos</span>
  </div>
  </div>


<div class="form-group">
 <label for="accountPhone" class="col-sm-2 control-label" >Telefone de contato</label>

  <div class="col-sm-10">
  <input type="text" name="accountPhone" id="accountPhone" value="<%- accountPhone %>"  class="form-control" aria-describedby="helpBlock">
  <span id="helpBlock" class="help-block">Opcional, mas nos ajuda a lhe retornar rapidamente</span>
</div>
  
</div>
</form>

  <div class="col-sm-offset-1 col-sm-11">
    <button class="btn btn-primary save_account">Salvar</button>
  </div>


  </div>
</div>
</script>


